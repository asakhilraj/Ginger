import MySQLdb

try:
	global db, cursor
	db=MySQLdb.connect("localhost","root","javabean22","SINGULARITY")
	cursor=db.cursor()
        
except Exception as err:
	print (err)



def store_in_db(name,place_id):
	try:
		sql_query=('INSERT INTO `hospital` (`name`,`place_id`) VALUES (%s,%s)')
		cursor.execute(sql_query,(name,place_id))
		print("%s,%s Inserted into the DB"%name %place_id)
	
	except Exception as err:
		print(err)

	return 0


result=open('response_json1.txt','r')


def parse_results(result):
	for r in result:
		r_list=r.get('results',None)
		for i in r_list:
			name=str(i.get('name',None))
			place_id=str(i.get('place_id',None))
			store_in_db(name,place_id)




